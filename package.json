{
  "name": "vscode-hacker-typer-fork",
  "displayName": "VSCode HackerTyper modified by nodename",
  "description": "Hacker Typer extension for looking cool while live coding",
  "keywords": [
    "vscode"
  ],
  "version": "0.2.3",
  "publisher": "nodename",
  "author": {
    "name": "Jani Ev√§kallio"
  },
  "contributors": [
    {
      "name": "Alan Shaw",
      "email": "alan@nodename.com"
    },
    {
      "name": "Kael Kirk",
      "email": "kirkkael@gmail.com"
    }
  ],
  "license": "MIT",
  "engines": {
    "vscode": "^1.27.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nodename/vscode-hacker-typer"
  },
  "icon": "docs/hackertyper-icon.png",
  "galleryBanner": {
    "color": "#1D1D1D",
    "theme": "dark"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:nodename.vscode-hacker-typer-fork.recordMacro",
    "onCommand:nodename.vscode-hacker-typer-fork.playMacro",
    "onCommand:nodename.vscode-hacker-typer-fork.deleteMacro",
    "onCommand:nodename.vscode-hacker-typer-fork.exportMacro",
    "onCommand:nodename.vscode-hacker-typer-fork.importMacro"
  ],
  "main": "./out/extension",
  "contributes": {
    "keybindings": [
      {
        "command": "nodename.vscode-hacker-typer-fork.recordMacro",
        "when": "isMac && editorTextFocus",
        "key": "cmd+shift+t r"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.recordMacro",
        "when": "isWindows && editorTextFocus",
        "key": "ctrl+shift+t r"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.deleteMacro",
        "when": "isMac",
        "key": "cmd+shift+t d"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.deleteMacro",
        "when": "isWindows",
        "key": "ctrl+shift+t d"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.exportMacro",
        "when": "isMac",
        "key": "cmd+shift+t e"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.exportMacro",
        "when": "isWindows",
        "key": "ctrl+shift+t e"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.importMacro",
        "when": "isMac",
        "key": "cmd+shift+t m"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.importMacro",
        "when": "isWindows",
        "key": "ctrl+shift+t m"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.playMacro",
        "when": "isMac",
        "key": "cmd+shift+t p"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.playMacro",
        "when": "isWindows",
        "key": "ctrl+shift+t p"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.saveOrDiscardMacro",
        "when": "isMac",
        "key": "cmd+shift+t s"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.saveOrDiscardMacro",
        "when": "isWindows",
        "key": "ctrl+shift+t s"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.cancelRecording",
        "when": "isMac",
        "key": "cmd+shift+t c"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.cancelRecording",
        "when": "isWindows",
        "key": "ctrl+shift+t c"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.insertStop",
        "when": "isMac",
        "key": "cmd+shift+t i"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.insertStop",
        "when": "isWindows",
        "key": "ctrl+shift+t i"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.undo",
        "when": "isMac",
        "key": "cmd+shift+t u"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.undo",
        "when": "isWindows",
        "key": "ctrl+shift+t u"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.cancelPlaying",
        "when": "isMac",
        "key": "cmd+shift+t x"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.cancelPlaying",
        "when": "isWindows",
        "key": "ctrl+shift+t x"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.toggleSilence",
        "when": "isMac",
        "key": "cmd+shift+t t"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.toggleSilence",
        "when": "isWindows",
        "key": "ctrl+shift+t t"
      }
    ],
    "commands": [
      {
        "command": "nodename.vscode-hacker-typer-fork.recordMacro",
        "title": "Record Macro",
        "category": "HackerTyper"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.playMacro",
        "title": "Play Macro",
        "category": "HackerTyper"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.saveOrDiscardMacro",
        "title": "Save or Discard Macro",
        "category": "HackerTyper"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.cancelRecording",
        "title": "Cancel Recording",
        "category": "HackerTyper"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.exportMacro",
        "title": "Export Macro",
        "category": "HackerTyper"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.importMacro",
        "title": "Import Macro",
        "category": "HackerTyper"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.deleteMacro",
        "title": "Delete Macro",
        "category": "HackerTyper"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.insertStop",
        "title": "Insert Soft Stop Point",
        "category": "HackerTyper"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.undo",
        "title": "Undo Last Recorded Buffer",
        "category": "HackerTyper"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.cancelPlaying",
        "title": "Cancel Playing",
        "category": "HackerTyper"
      },
      {
        "command": "nodename.vscode-hacker-typer-fork.toggleSilence",
        "title": "Toggle Silence",
        "category": "HackerTyper"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "node": "14.0.0",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.53",
    "tslint": "^5.20.1",
    "typescript": "^3.6.2",
    "vscode": "1.1.37"
  },
  "dependencies": {
    "xstate": "4.9.1",
    "js-csp": "^1.0.1",
    "music-metadata": "^4.4.0",
    "strtok3": "6.0.0",
    "node-wav-player": "^0.1.0",
    "on-change": "^2.0.1",
    "token-types": "1.0.2",
    "vscode-cache": "^0.3.0"
  },
  "WARNING": "All occurrences of strtok3/lib/type in music-metadata source files must be changed to strtok3/lib/types"
}
